    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.4.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^1.1.8",
    "html-webpack-plugin": "^2.30.1",
